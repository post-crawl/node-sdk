{
	"name": "postcrawl",
	"version": "0.1.0",
	"description": "Node.js/TypeScript SDK for PostCrawl - The Fastest LLM Ready Social Media Crawler",
	"keywords": [
		"postcrawl",
		"social-media",
		"scraping",
		"reddit",
		"tiktok",
		"api",
		"sdk",
		"typescript",
		"nodejs"
	],
	"homepage": "https://postcrawl.com/",
	"bugs": {
		"url": "https://github.com/post-crawl/node-sdk/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/post-crawl/node-sdk.git"
	},
	"license": "MIT",
	"author": "PostCrawl Team <support@postcrawl.com>",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"check:biome": "biome check .",
		"check:biome:fix": "biome check --write .",
		"check:types": "tsc --noEmit",
		"check": "bun run check:biome && bun run check:types && bun run test",
		"ci": "biome ci .",
		"prepublishOnly": "bun run build",
		"examples": "bun run example:search && bun run example:extract && bun run example:sne",
		"example:search": "tsx examples/search_101.ts",
		"example:extract": "tsx examples/extract_101.ts",
		"example:sne": "tsx examples/search_and_extract_101.ts"
	},
	"dependencies": {
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.0",
		"@types/node": "^20.10.0",
		"@vitest/coverage-v8": "^1.0.0",
		"dotenv": "^16.4.5",
		"tsup": "^8.0.1",
		"tsx": "^4.7.0",
		"typescript": "^5.3.0",
		"vitest": "^1.0.0"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
